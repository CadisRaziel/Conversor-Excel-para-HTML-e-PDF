<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>TabelaT</title>
</head>
<body>

    <table>
        <thead>
            <% header.forEach(col => { %>
                <th> <%= col %> </th>
            <% }) %>
        </thead>
        <tbody>
            <% row.forEach(rows => { %>
                <tr>
                    <% rows.forEach(element => { %>
                        <td> <%= element %> </td>
                    <% }) %>
                </tr>
            <% }) %>
        </tbody>
    </table>

</body>
</html>